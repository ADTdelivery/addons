<odoo>
    <record id="group_expediente_user" model="res.groups">
        <field name="name">Usuario Expedientes</field>
    </record>

    <record id="group_expediente_supervisor" model="res.groups">
        <field name="name">Supervisor Expedientes</field>
    </record>

    <!-- Permisos de acceso para FCM Device -->
    <record id="access_adt_fcm_device" model="ir.model.access">
        <field name="name">access_adt_fcm_device</field>
        <field name="model_id" ref="model_adt_fcm_device"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>
</odoo>
