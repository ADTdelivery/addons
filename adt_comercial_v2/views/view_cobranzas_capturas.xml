    <record id="adt_reporte_cobranza_recuperado_v2_search" model="ir.ui.view">
        <field name="name">adt.reporte.cobranza.captura.v2.search</field>
        <field name="model">adt.reporte.cobranza.recuperado.v2</field>
        <field name="arch" type="xml">
            <search string="Search Partner">
                <field name="partner_id" />
                <field name="cuenta_id"/>
                <field name="vehiculo_id"/>
                <separator/>
                <group expand="0" name="group_by" string="Group By">
                    <filter name="cuenta" string="Cuenta" domain="[]" context="{'group_by' : 'cuenta_id'}"/>
                    <filter name="partner" string="Socio" domain="[]" context="{'group_by' : 'partner_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="action_cobranzas_recuperado_v2" model="ir.actions.act_window">
        <field name="name">Cobranzas - Recuperado</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">adt.reporte.cobranza.recuperado.v2</field>
        <field name="view_mode">tree</field>
        <field name="search_view_id" ref="adt_comercial_v2.adt_reporte_cobranza_recuperado_v2_search"/>
    </record>

    <record id="action_cobranzas_recuperado_view_tree" model="ir.actions.act_window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="adt_comercial_v2.adt_reporte_cobranza_recuperado_v2_tree"/>
        <field name="act_window_id" ref="action_cobranzas_recuperado_v2"/>
    </record>

    <menuitem id="adt_cobranza_menu_capturas" name="Capturas" parent="menu_adt_cobranza" sequence="3"/>
    <menuitem id="adt_cobranza_menu_en_captura" name="En captura" parent="adt_cobranza_menu_capturas" action="action_cobranzas_en_captura_v2" sequence="1"/>
    <menuitem id="adt_cobranza_menu_recuperado" name="Recuperados" parent="adt_cobranza_menu_capturas" action="action_cobranzas_recuperado_v2" sequence="1"/>

</odoo>