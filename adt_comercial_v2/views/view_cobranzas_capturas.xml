<odoo>
    <record id="adt_reporte_cobranza_en_captura_v2_tree" model="ir.ui.view">
        <field name="name">adt.reporte.cobranza.en.captura.v2.tree</field>
        <field name="model">adt.reporte.cobranza.captura.v2</field>
        <field name="arch" type="xml">
            <tree string="En captura" create="0">
                <field name="recuperado"/>
                <field name="partner_id"/>
                <field name="cuenta_id" string="Cuenta"/>
                <field name="dias_retraso"/>
                <field name="periodicidad"/>
                <field name="fecha_cronograma"/>
                <field name="monto"/>
                <field name="gps_chip"/>
                <field name="gps_activo"/>
            </tree>
        </field>
    </record>

    <record id="adt_reporte_cobranza_en_captura_v2_search" model="ir.ui.view">
        <field name="name">adt.reporte.cobranza.captura.v2.search</field>
        <field name="model">adt.reporte.cobranza.captura.v2</field>
        <field name="arch" type="xml">
            <search string="Search Partner">
                <field name="partner_id" />
                <field name="cuenta_id"/>
                <separator/>
                <filter string="Fecha de cronograma" name="filter_fecha_cronograma" date="fecha_cronograma"/>
                <separator/>
                <group expand="0" name="group_by" string="Group By">
                    <filter name="fecha" string="Fecha de cronograma" domain="[]" context="{'group_by' : 'fecha_cronograma'}"/>
                    <filter name="cuenta" string="Cuenta" domain="[]" context="{'group_by' : 'cuenta_id'}"/>
                    <filter name="partner" string="Socio" domain="[]" context="{'group_by' : 'partner_id'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="action_cobranzas_en_captura_v2" model="ir.actions.act_window">
        <field name="name">Cobranzas - En captura</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">adt.reporte.cobranza.captura.v2</field>
        <field name="view_mode">tree</field>
        <field name="domain">[('recuperado','=',False)]</field>
        <field name="search_view_id" ref="adt_comercial_v2.adt_reporte_cobranza_en_captura_v2_search"/>
        <!-- <field name="context">{'default_is_company': True}</field> -->
    </record>

    <record id="action_cobranzas_en_captura_v2_view_tree" model="ir.actions.act_window.view">
        <field name="sequence" eval="1"/>
        <field name="view_mode">tree</field>
        <field name="view_id" ref="adt_comercial_v2.adt_reporte_cobranza_en_captura_v2_tree"/>
        <field name="act_window_id" ref="action_cobranzas_en_captura_v2"/>
    </record>

    <record id="adt_reporte_cobranza_recuperado_v2_tree" model="ir.ui.view">
        <field name="name">adt.reporte.cobranza.recuperado.v2.tree</field>
        <field name="model">adt.reporte.cobranza.recuperado.v2</field>
        <field name="arch" type="xml">
            <tree string="En captura" create="0">
                <field name="recuperado"/>
                <field name="partner_id"/>
                <field name="cuenta_id" string="Cuenta"/>
                <field name="vehiculo_id"/>
            </tree>
        </field>
    </record>

