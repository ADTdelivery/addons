<odoo>
<record id="adt_fleet_vehicle_form1" model="ir.ui.view">
    <field name="name">adt.fleet.vehicle.form1</field>
    <field name="model">fleet.vehicle</field>
    <field name="inherit_id" ref="fleet.fleet_vehicle_view_form" />
    <field name="arch" type="xml">
        <xpath expr="//notebook" position="inside">
            <page string="Tramite de placas">
                <group>

                    <field name="nombre_titular"/>
                    <field name="nombre_proveedor"/>
                    <field name="num_factura"/>


                </group>
            </page>
        </xpath>
    </field>
</record>
</odoo>